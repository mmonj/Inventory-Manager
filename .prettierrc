{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "printWidth": 100,
  "endOfLine": "auto",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react",
    "^react/(.*)$",
    "^@reactivated",
    "^@core/(.*)$",
    "^next",
    "^@fortawesome",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^@client/(.*)$",
    "^@[^/]+",
    "^[a-zA-Z]",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
